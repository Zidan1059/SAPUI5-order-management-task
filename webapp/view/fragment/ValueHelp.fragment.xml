<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">


    <TableSelectDialog confirm=".onCloseDialog" cancel=".onCloseDialog" title="Select Customer" items="{
            path : '/helpList'
           
        }" search=".handleSearch">

        <ColumnListItem selected="{selected}">
            <cells>

                <Text text="{customer}" />
                <ObjectIdentifier title="{customerName}" />
                <Text text="{email}" />
                <Text text="{mobile}" />

            </cells>
        </ColumnListItem>
        <columns>
            <Column>
                <header>
                    <Text text="{i18n>customer}" />
                </header>
            </Column>
            <Column>
                <header>
                    <Text text="{i18n>formcustomername}" />
                </header>
            </Column>
            <Column>
                <header>
                    <Text text="{i18n>email}" />
                </header>
            </Column>
            <Column>
                <header>
                    <Text text="{i18n>mobile}" />
                </header>
            </Column>
        </columns>
    </TableSelectDialog>


</core:FragmentDefinition>